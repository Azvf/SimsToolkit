<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style Selector="Window">
        <Setter Property="Background" Value="{DynamicResource Brush.AppBackground}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextPrimary}" />
    </Style>

    <Style Selector="Border.card">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Medium}" />
    </Style>

    <Style Selector="Border.card-muted">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="Border.tray-preview-item-card">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
    </Style>

    <Style Selector="Border.tray-preview-item-card.selected">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
        <Setter Property="BorderThickness" Value="1.5" />
    </Style>

    <Style Selector="Border.tray-export-queue">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
        <Setter Property="BorderThickness" Value="1" />
    </Style>

    <Style Selector="Border.tray-export-queue-dock">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
    </Style>

    <Style Selector="Border.tray-export-task-card.running">
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Border.tray-export-task-card.failed">
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Danger}" />
        <Setter Property="BorderThickness" Value="1.5" />
    </Style>

    <Style Selector="Border.tray-preview-empty-state">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderHover}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
        <Setter Property="Padding" Value="20" />
    </Style>

    <Style Selector="Border.sidebar-shell">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Medium}" />
    </Style>

    <Style Selector="Border.sidebar-hero">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Medium}" />
    </Style>

    <Style Selector="Border.sidebar-block, Border.sidebar-cta-block">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Medium}" />
    </Style>

    <Style Selector="Border.sidebar-subtle-block">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="Border.path-health-interactive">
        <Setter Property="Cursor" Value="Hand" />
    </Style>

    <Style Selector="Border.path-health-interactive:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Border.path-health-interactive:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfacePressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Border.inspector-shell">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Medium}" />
    </Style>

    <Style Selector="Border.inspector-header, Border.inspector-empty, Border.inspector-detail">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="TextBox, ComboBox, Button">
        <Setter Property="Height" Value="{DynamicResource Size.ControlHeight}" />
    </Style>

    <Style Selector="TextBox, ComboBox">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextPrimary}" />
    </Style>

    <Style Selector="TextBox:pointerover, ComboBox:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderHover}" />
    </Style>

    <Style Selector="TextBox:focus, ComboBox:focus, TextBox:focus-within, ComboBox:focus-within">
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="TextBox.path-field">
        <Setter Property="FontFamily" Value="Consolas, Cascadia Mono, JetBrains Mono" />
    </Style>

    <Style Selector="Border.settings-readonly-field">
        <Setter Property="Height" Value="{DynamicResource Size.ControlHeight}" />
        <Setter Property="Padding" Value="12,0" />
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="Border.settings-section">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="TextBlock.settings-readonly-value">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontFamily" Value="Consolas, Cascadia Mono, JetBrains Mono" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextSecondary}" />
        <Setter Property="TextTrimming" Value="CharacterEllipsis" />
    </Style>

    <Style Selector="TextBox.log-output">
        <Setter Property="FontFamily" Value="Consolas, Cascadia Mono, JetBrains Mono" />
        <Setter Property="Background" Value="{DynamicResource Brush.LogBackground}" />
    </Style>

    <Style Selector="Button">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextPrimary}" />
    </Style>

    <Style Selector="Button:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderHover}" />
    </Style>

    <Style Selector="Button:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfacePressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button:focus, Button:focus-visible">
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button.primary">
        <Setter Property="Background" Value="{DynamicResource Brush.Accent}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Accent}" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>

    <Style Selector="Button.primary:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.AccentHover}" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style Selector="Button.primary:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentPressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.AccentPressed}" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style Selector="Button.primary:focus, Button.primary:focus-visible">
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.AccentHover}" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style Selector="Button.secondary">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
    </Style>

    <Style Selector="Button.secondary:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMutedHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button.secondary:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMutedPressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button.secondary:focus, Button.secondary:focus-visible">
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button.nav-active">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Accent}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.Accent}" />
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>

    <Style Selector="Button.nav-active:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMutedHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.AccentHover}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.Accent}" />
    </Style>

    <Style Selector="Button.nav-active:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMutedPressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.AccentPressed}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.Accent}" />
    </Style>

    <Style Selector="Button.sidebar-link">
        <Setter Property="MinHeight" Value="50" />
        <Setter Property="Padding" Value="10,8" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="Button.sidebar-link:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button.sidebar-mini-link">
        <Setter Property="Height" Value="32" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="Padding" Value="10,2" />
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
    </Style>

    <Style Selector="Button.sidebar-mini-link:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="Button.sidebar-link-minimal">
        <Setter Property="Height" Value="40" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="10,0,10,0" />
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
    </Style>

    <Style Selector="Button.sidebar-link-minimal:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderHover}" />
    </Style>

    <Style Selector="Button.sidebar-active">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextPrimary}" />
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>

    <Style Selector="Button.sidebar-active:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextPrimary}" />
    </Style>

    <Style Selector="Button.sidebar-launch">
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>

    <Style Selector="Button.sidebar-launch-minimal">
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="CornerRadius" Value="{DynamicResource Radius.Small}" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>

    <Style Selector="Button.inspector-action">
        <Setter Property="Height" Value="34" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
    </Style>

    <Style Selector="Border.status-pill">
        <Setter Property="Padding" Value="8,2" />
        <Setter Property="CornerRadius" Value="999" />
        <Setter Property="BorderThickness" Value="1" />
    </Style>

    <Style Selector="Border.status-pill.status-ok">
        <Setter Property="Background" Value="{DynamicResource Brush.AccentMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Success}" />
    </Style>

    <Style Selector="Border.status-pill.status-warning">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Warning}" />
    </Style>

    <Style Selector="Border.status-pill.status-missing">
        <Setter Property="Background" Value="{DynamicResource Brush.Surface}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Danger}" />
    </Style>

    <Style Selector="Border.status-dot">
        <Setter Property="Width" Value="8" />
        <Setter Property="Height" Value="8" />
        <Setter Property="CornerRadius" Value="999" />
        <Setter Property="Background" Value="{DynamicResource Brush.Border}" />
    </Style>

    <Style Selector="Border.status-dot.status-ok">
        <Setter Property="Background" Value="{DynamicResource Brush.Success}" />
    </Style>

    <Style Selector="Border.status-dot.status-warning">
        <Setter Property="Background" Value="{DynamicResource Brush.Warning}" />
    </Style>

    <Style Selector="Border.status-dot.status-missing">
        <Setter Property="Background" Value="{DynamicResource Brush.Danger}" />
    </Style>

    <Style Selector="Button.danger">
        <Setter Property="Background" Value="{DynamicResource Brush.Danger}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Danger}" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>

    <Style Selector="Button.danger:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.DangerHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.DangerHover}" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style Selector="Button.danger:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.DangerPressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.DangerPressed}" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style Selector="CheckBox">
        <Setter Property="Foreground" Value="{DynamicResource Brush.TextPrimary}" />
    </Style>

    <Style Selector="CheckBox:pointerover">
        <Setter Property="Foreground" Value="{DynamicResource Brush.Accent}" />
    </Style>

    <Style Selector="ProgressBar">
        <Setter Property="Foreground" Value="{DynamicResource Brush.Accent}" />
    </Style>

    <Style Selector="ListBox">
        <Setter Property="Background" Value="{DynamicResource Brush.SurfaceMuted}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.Border}" />
        <Setter Property="BorderThickness" Value="1" />
    </Style>

    <Style Selector="ListBoxItem:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.ListItemHover}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderHover}" />
    </Style>

    <Style Selector="ListBoxItem:pressed">
        <Setter Property="Background" Value="{DynamicResource Brush.ListItemPressed}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>

    <Style Selector="ListBoxItem:selected, ListBoxItem:selected:pointerover">
        <Setter Property="Background" Value="{DynamicResource Brush.ListItemSelected}" />
        <Setter Property="BorderBrush" Value="{DynamicResource Brush.BorderFocus}" />
    </Style>
</Styles>

