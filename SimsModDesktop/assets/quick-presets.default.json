{
  "version": 1,
  "presets": [
    {
      "id": "merge-fast",
      "name": "Merge Fast",
      "description": "Fast merge with conflict verification",
      "action": "Merge",
      "autoRun": true,
      "actionPatch": {
        "sourcePaths": [
          "D:\\Mods\\A",
          "D:\\Mods\\B"
        ],
        "targetPath": "D:\\Mods\\Merged"
      },
      "sharedPatch": {
        "modFilesOnly": true,
        "verifyContentOnNameConflict": true,
        "hashWorkerCount": 16
      }
    },
    {
      "id": "flatten-to-root",
      "name": "Flatten To Root",
      "description": "Flatten one folder into its root directory",
      "action": "Flatten",
      "autoRun": false,
      "actionPatch": {
        "flattenToRoot": true
      }
    }
  ]
}
